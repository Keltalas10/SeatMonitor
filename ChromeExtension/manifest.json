{
  "manifest_version": 3,
  "key": "",
  "name": "Seat Monitor",
  "version": "1.17",
  "description": "Автоматический мониторинг и клик по элементам с классом 'seat-undefined'",
  "permissions": ["activeTab", "scripting", "storage", "notifications", "tabs"],
  "host_permissions": ["https://game.r-gaming.com/*"],
  "content_scripts": [
    {
      "matches": ["https://game.r-gaming.com/*"],
      "js": [
        "js/config.js",
        "js/auth.js",
        "js/elementUtils.js",
        "js/clickHandler.js",
        "js/popupHandler.js",
        "js/availableMonitor.js",
        "js/targetPlayerMonitor.js",
        "js/monitor.js",
        "js/messageHandler.js",
        "js/content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "html/main/popup.html",
    "default_title": "Seat Monitor"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}
